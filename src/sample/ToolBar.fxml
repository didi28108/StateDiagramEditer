<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.Color?>


<ToolBar prefHeight="40.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
  <items>
    <Button fx:id="btn_new" mnemonicParsing="false" onAction="#new_press" text="NEW" />
      <Button fx:id="btn_load" disable="true" mnemonicParsing="false" text="LOAD" />
      <Pane prefHeight="40.0" prefWidth="40.0" />
      <Button fx:id="btn_undo" mnemonicParsing="false" onAction="#undo_press" text="UNDO" />
      <Button fx:id="btn_redo" mnemonicParsing="false" onAction="#redo_press" text="REDO" />
      <Pane prefHeight="40.0" prefWidth="40.0" />
      <Button fx:id="btn_edit" mnemonicParsing="false" onAction="#edit_press" text="EDIT" />
      <TextField fx:id="textField" />
      <Pane prefHeight="40.0" prefWidth="40.0" />
      <Button fx:id="btn_delete" mnemonicParsing="false" onAction="#delete_press" text="DELETE" />
      <Pane prefHeight="40.0" prefWidth="40.0" />
      <Button fx:id="btn_state" mnemonicParsing="false" onAction="#state_press" text="STATE" />
      <Button fx:id="btn_transition" mnemonicParsing="false" onAction="#transition_press" text="TRANSITION" />
      <ColorPicker fx:id="main_colorPicker">
         <value>
            <Color blue="0.0" green="0.0" red="0.0" /> </value>
      </ColorPicker>
      <ColorPicker fx:id="text_colorPicker">
         <value>
            <Color blue="0.0" green="0.0" red="0.0" /> </value>
      </ColorPicker>
  </items>
</ToolBar>
